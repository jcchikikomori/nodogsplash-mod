{
  "name": "nodogsplash-mod",
  "version": "1.0.0",
  "description": "Custom Splash Screen for nodogsplash",
  "main": "nodogsplash\\htdocs\\splash.html",
  "scripts": {
    "styleLegacy": "node-sass --importer node_modules/node-sass-import src/scss/main.scss htdocs/splash.css && ",
    "clean": "rimraf scss",
    "compile": "node-sass --importer node_modules/node-sass-import --output-style=expanded --source-map=true src/scss/main.scss htdocs/splash.css",
    "prefix": "postcss htdocs/splash.css --use=autoprefixer --map=false --output=htdocs/splash.css",
    "minify": "cleancss --source-map --source-map-inline-sources --output htdocs/splash.min.css htdocs/splash.css",
    "dev": "npm run compile -- --watch",
    "webpack": "webpack --config webpack.config.js --mode production",
    "webpackDebug": "webpack --config webpack.config.js --mode development",
    "build": "npm run clean && npm run compile && npm run prefix && npm run minify",
    "dist": "npm run clean && npm run compile && npm run prefix && npm run minify",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jcchikikomori/nodogsplash-mod.git"
  },
  "keywords": [
    "captive",
    "portal",
    "nodogsplash",
    "splashscreen"
  ],
  "author": "jcchikikomori",
  "private": true,
  "dependencies": {
    "animate.css": "^4.1.1",
    "bulma": "^1.0.0",
    "bulma-dark": "^0.0.2",
    "css-skeletons": "^1.0.7",
    "include-media": "^1.4.10",
    "node-sass-import": "^2.0.1",
    "sass": "^1.57.0",
    "sass-convert": "^0.5.2"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@babel/preset-env": "^7.21.4",
    "autoprefixer": "^10.4.14",
    "babel-loader": "^9.1.2",
    "clean-css-cli": "^5.6.2",
    "css-has-pseudo": "^5.0.2",
    "eslint": "^8.38.0",
    "eslint-plugin-jquery": "^1.5.1",
    "jquery": "^3.6.4",
    "less": "^4.1.3",
    "lessc": "^1.0.2",
    "node-sass": "^8.0.0",
    "normalize-scss": "^7.0.1",
    "postcss": "^8.4.21",
    "postcss-cli": "^10.1.0",
    "postcss-load-config": "^4.0.1",
    "uglify-js": "^3.17.4",
    "webpack": "^5.79.0",
    "webpack-cli": "^5.0.1"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jcchikikomori/nodogsplash-mod/issues"
  },
  "homepage": "https://github.com/jcchikikomori/nodogsplash-mod#readme"
}